services:
  webapplication1:
    build:
      context: C:\Development\R&D\.Net 6 Docker\WebAppSample
      dockerfile: WebApplication1/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApplication1
      target: base
    container_name: WebApplication1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: webapplication1:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/WebApplication1.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Development\R&D\.Net 6 Docker\WebAppSample\WebApplication1:/app:rw
    - C:\Users\Pedro.VilaVerde\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Pedro.VilaVerde\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Pedro.VilaVerde\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\Pedro.VilaVerde\.nuget\packages:/root/.nuget/packages:ro
    - C:\Development\R&D\.Net 6 Docker\WebAppSample:/src:rw
version: '3.4'